if getgenv().MercenariesLoaded then
    return
end
getgenv().MercenariesLoaded = true

if not game:IsLoaded() then
	game.Loaded:Wait()
end

task.wait(math.random())

if game.CreatorId == 34088633 then
	loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/a2b73ff8c5bf48a0dce49b0bbf496cad.lua"))()
end
